(mod (BOARD V pos)
    (include tic-tac-toe.clib)

    (defun check-new-board (new_board V)
        (list
            (check-board
                new_board
                V
            )
            new_board
        )
    )

    (check-new-board
        (new-board-rec BOARD V 0 pos)
        V
    )
)