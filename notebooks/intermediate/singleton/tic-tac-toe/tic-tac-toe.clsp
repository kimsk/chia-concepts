(mod (BOARD_HASH board (pos v))
    (include tic-tac-toe.clib)

    (defun check-new-board (new_board v)
        (list
            (check-board
                new_board
                v
            )
            new_board
        )
    )

    (if (= BOARD_HASH (sha256tree board))
        (check-new-board
            (new-board-rec board 0 pos v)
            v
        )
        (x)
    )
    
)